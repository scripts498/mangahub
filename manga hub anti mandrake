end)

local AudioTab = GUI:Tap("Audio FE (Beta)")

local audioID = ""
local selectedPlayerName = ""
local looping = false

AudioTab:TextBox("ID Do Audio (obrigat√≥rio)", function(value)
    audioID = value
end)

AudioTab:Button("Pegar Arma (Obrigat√≥rio)", function()
    local args = {
        [1] = "PickingTools",
        [2] = "Sniper"
    }
    game:GetService("ReplicatedStorage").RE:FindFirstChild("1Too1l"):InvokeServer(unpack(args))
end)

AudioTab:Button("Tocar Audio", function()
    local sniper = game:GetService("Players").LocalPlayer.Character:FindFirstChild("Sniper")
    if sniper and sniper:FindFirstChild("Handle") and audioID and tonumber(audioID) then
        local args = {
            [1] = sniper.Handle,
            [2] = tonumber(audioID),
            [3] = 1
        }
        game:GetService("ReplicatedStorage").RE:FindFirstChild("1Gu1nSound1s"):FireServer(unpack(args))
        local sound = game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Sound")
        if sound then
            task.wait(3)
            sound:Stop()
        end
    else
        warn("Sniper n√£o encontrado ou ID de √°udio inv√°lido!")
    end
end)

AudioTab:Button("Tocar audio globalmente (todos ouvem, voc√™ tamb√©m.)", function()
    if audioID and tonumber(audioID) then
        local args = {
            [1] = game:GetService("Workspace"),
            [2] = tonumber(audioID),
            [3] = 1,
        }
        game:GetService("ReplicatedStorage").RE:FindFirstChild("1Gu1nSound1s"):FireServer(unpack(args))
        local sound = Instance.new("Sound")
        sound.SoundId = "rbxassetid://"..audioID
        sound.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
        sound:Play()
        task.wait(3)
        sound:Stop()
    else
        warn("ID do √°udio inv√°lido!")
    end
end)

AudioTab:TextBox("Nome do jogador", function(value)
    selectedPlayerName = value
end)

AudioTab:Button("Tocar √°udio no player selecionado (100% FE)", function()
    if selectedPlayerName then
        local targetPlayer = game.Players:FindFirstChild(selectedPlayerName)
        if targetPlayer then
            local soundId = audioID
            local args = {
                [1] = targetPlayer.Character:FindFirstChild("Head"),
                [2] = tonumber(soundId),
                [3] = 1
            }
            game:GetService("ReplicatedStorage").RE:FindFirstChild("1Gu1nSound1s"):FireServer(unpack(args))
            local sound = Instance.new("Sound")
            sound.SoundId = "rbxassetid://" .. soundId
            sound.Parent = targetPlayer.Character.Head
            sound:Play()
            task.wait(3)
            sound:Stop()
        else
            warn("Jogador n√£o encontrado!")
        end
    else
        warn("Selecione um jogador primeiro!")
    end
end)

AudioTab:Toggle("Tocar Audio em Loop", nil, function(value)
    looping = value
    if looping then
        local function loopAudioGlobal()
            if looping and audioID and tonumber(audioID) then
                local args = {
                    [1] = game:GetService("Workspace"),
                    [2] = tonumber(audioID),
                    [3] = 1
                }
                game:GetService("ReplicatedStorage").RE:FindFirstChild("1Gu1nSound1s"):FireServer(unpack(args))
                task.wait(3)
                loopAudioGlobal()
            end
        end
        loopAudioGlobal()
    end
end)

