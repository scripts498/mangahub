local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()

local windowSize = isMobile and UDim2.fromOffset(400, 300) or UDim2.fromOffset(580, 460)
local tabWidth = isMobile and 120 or 160

local Window = Fluent:CreateWindow({
    Title = "Sistema de Votação",
    SubTitle = "BBB Brasil",
    TabWidth = tabWidth,
    Size = windowSize,
    Acrylic = true,
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.LeftControl
})

getgenv().MangaHubWindow = Window

local Tabs = {
    Main = Window:AddTab({ Title = "Votação", Icon = "" })
}

local Options = Fluent.Options

local selectedPlayer = nil
local votingEnabled = false
local votingCoroutine = nil
local playerList = {}

local function getPlayerList()
    local list = {}
    for _, player in pairs(Players:GetPlayers()) do
        if player ~= Players.LocalPlayer then
            table.insert(list, player.Name)
        end
    end
    return list
end

local function votePlayer(playerName)
    local args = {
        [1] = playerName,
        [2] = 5
    }
    
    pcall(function()
        ReplicatedStorage.Remotes:FindFirstChild("\240\159\140\131\240\159\149\139\240\159\154\135\226\152\132"):FireServer(unpack(args))
    end)
end

playerList = getPlayerList()

local Dropdown = Tabs.Main:AddDropdown("PlayerDropdown", {
    Title = "Selecionar Jogador",
    Values = playerList,
    Multi = false,
    Default = 1,
})

Dropdown:OnChanged(function(Value)
    selectedPlayer = Value
end)

Tabs.Main:AddButton({
    Title = "Atualizar Lista",
    Description = "Atualiza a lista de jogadores",
    Callback = function()
        playerList = getPlayerList()
        Dropdown:SetValues(playerList)
        Fluent:Notify({
            Title = "Lista Atualizada",
            Content = "A lista de jogadores foi atualizada!",
            Duration = 3
        })
    end
})

local Toggle = Tabs.Main:AddToggle("VoteToggle", {Title = "Votar nesta pessoa", Default = false})

Toggle:OnChanged(function()
    votingEnabled = Options.VoteToggle.Value
    
    if votingEnabled then
        if not selectedPlayer or selectedPlayer == "" then
            Fluent:Notify({
                Title = "Erro",
                Content = "Selecione um jogador primeiro!",
                Duration = 5
            })
            Options.VoteToggle:SetValue(false)
            return
        end
        
        votingCoroutine = coroutine.create(function()
            while votingEnabled do
                if selectedPlayer and selectedPlayer ~= "" then
                    votePlayer(selectedPlayer)
                end
                task.wait(0.)
            end
        end)
        
        coroutine.resume(votingCoroutine)
        
        Fluent:Notify({
            Title = "Votação Iniciada",
            Content = "Votando em: " .. selectedPlayer,
            Duration = 3
        })
    else
        votingEnabled = false
        Fluent:Notify({
            Title = "Votação Parada",
            Content = "A votação foi interrompida!",
            Duration = 3
        })
    end
end)

Tabs.Main:AddParagraph({
    Title = "Informações",
    Content = "Você deve usar isso em pessoas no paredão.\n\nQuando ativado em uma pessoa no paredão, tem 98% de chance da pessoa ir embora, e 2% dela não ir."
})

SaveManager:SetLibrary(Fluent)
InterfaceManager:SetLibrary(Fluent)
SaveManager:IgnoreThemeSettings()
SaveManager:SetIgnoreIndexes({})
InterfaceManager:SetFolder("VotingSystem")
SaveManager:SetFolder("VotingSystem/config")

Window:SelectTab(1)

Fluent:Notify({
    Title = "Sistema de Votação",
    Content = "Script carregado com sucesso!",
    Duration = 5
})
