local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/jensonhirst/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "manga hub", HidePremium = false, SaveConfig = false, IntroEnabled = true, IntroText = "bem vindo!"})

local AdminTab = Window:MakeTab({
	Name = "admin ( FE )",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

AdminTab:AddButton({
	Name = "pegar admin",
	Callback = function()
        game:GetService("ReplicatedStorage").AdminRankEvent:FireServer()
	end    
})

OrionLib:Init()
